    "env-cmd": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "postcss": "^8.4.47",
    "prettier": "2.8.3",
    "tailwindcss": "^3.4.14"
  }
